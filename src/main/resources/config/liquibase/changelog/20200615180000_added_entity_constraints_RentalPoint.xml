<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity RentalPoint.
    -->
    <changeSet id="20200615180000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="autopark_id"
                                 baseTableName="rental_point"
                                 constraintName="fk_rental_point_autopark_id"
                                 referencedColumnNames="id"
                                 referencedTableName="autopark"/>

        <addForeignKeyConstraint baseColumnNames="rental_point_id"
                                 baseTableName="rental_point_clients"
                                 constraintName="fk_rental_point_clients_rental_point_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rental_point"/>

        <addForeignKeyConstraint baseColumnNames="clients_id"
                                 baseTableName="rental_point_clients"
                                 constraintName="fk_rental_point_clients_clients_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>
        
    </changeSet>
</databaseChangeLog>

        <addForeignKeyConstraint baseColumnNames="rent_id"
                                 baseTableName="fine"
                                 constraintName="fk_fine_rent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rent"/>

    </changeSet>
</databaseChangeLog>

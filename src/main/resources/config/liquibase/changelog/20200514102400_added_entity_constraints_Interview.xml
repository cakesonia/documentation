<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Interview.
    -->
    <changeSet id="20200514102400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="result_id"
                                 baseTableName="interview"
                                 constraintName="fk_interview_result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="interview_result"/>

        <addForeignKeyConstraint baseColumnNames="candidate_id"
                                 baseTableName="interview"
                                 constraintName="fk_interview_candidate_id"
                                 referencedColumnNames="id"
                                 referencedTableName="candidate"/>

        <addForeignKeyConstraint baseColumnNames="interviewer_id"
                                 baseTableName="interview"
                                 constraintName="fk_interview_interviewer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="interviewer"/>

    </changeSet>
</databaseChangeLog>
